var name = prompt("Please enter your name", "Michael Phelps");
var request = "SELECT ?human ?count WHERE{SELECT ?human (count(*) as ?count) WHERE {?event rdf:type dbo:OlympicEvent{?event dbo:bronzeMedalist ?human .  } UNION {?event dbo:silverMedalist ?human} UNION {?event dbo:goldMedalist ?human}?human dbp:name ?nam.FILTER(str(?nam) = \""+ name +"\").}GROUP BY ?human}ORDER BY DESC(?count)";
var result = String(get_url(request));
document.writeln(result);
